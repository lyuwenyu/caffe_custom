net: "googlenet_train.prototxt"

test_iter: 200
test_interval: 500
test_initialization: false

display: 20
average_loss: 20

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 3000   # TODO 4000
max_iter: 9000  # TODO 12000
momentum: 0.96
weight_decay: 0.0005

snapshot: 3000 #2000
snapshot_prefix: "outputs/multi_view_test_v1" #TODO

solver_mode: GPU
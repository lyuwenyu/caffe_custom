gamma: 0.1
stepsize: 1500   # TODO
max_iter: 6000  # TODO
momentum: 0.96
weight_decay: 0.0005

snapshot: 500
snapshot_prefix: "outputsx/resnet_101_outsiderear_withtest" #TODO

solver_mode: GPU